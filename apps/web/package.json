{
  "name": "@jinho-blog/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "copy-images": "pnpm --filter @jinho-blog/mdx-handler run copy-images",
    "build-registry": "pnpm --filter @jinho-blog/mdx-handler run build-registry",
    "registry": "pnpm run copy-images && pnpm run build-registry",
    "dev": "pnpm run registry && set PORT=3401 && next dev",
    "build": "pnpm run registry && next build",
    "start": "set PORT=3401 && next start",
    "lint": "concurrently \"pnpm run lf\" \"pnpm run eslint\" \"pnpm run type-check\"",
    "lf": "prettier --write -l --end-of-line lf src",
    "eslint": "eslint src --fix --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc --noEmit",
    "generate-routes": "nextjs-routes"
  },
  "dependencies": {
    "@jinho-blog/mdx-handler": "workspace:*",
    "@jinho-blog/shared": "workspace:*",
    "@vercel/analytics": "^1.6.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gray-matter": "^4.0.3",
    "motion": "^12.23.24",
    "next": "^16.1.6",
    "next-mdx-remote-client": "^2.1.7",
    "postcss": "^8.5.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-error-boundary": "^6.0.0",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-slug": "^6.0.0",
    "remark-flexible-toc": "^1.2.4",
    "remark-gfm": "^4.0.1",
    "sanitize-html": "^2.17.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jinho-blog/nextjs-routes": "workspace:*",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^20",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/sanitize-html": "^2.16.0",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "^16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-better-tailwindcss": "^3.7.10",
    "eslint-plugin-boundaries": "^5.3.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "postcss-import": "^16.1.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
