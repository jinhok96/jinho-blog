{
  "name": "jinho-blog-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "npm run start -w @jinho-blog/web",
    "lint": "turbo lint",
    "lf": "turbo lf",
    "eslint": "turbo eslint",
    "type-check": "turbo type-check",
    "kill-port": "for /f \"tokens=5\" %a in ('netstat -ano ^| findstr :3401 ^| findstr LISTENING') do taskkill /F /PID %a"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.7.3",
    "typescript": "^5"
  }
}
