{
  "name": "jinho-blog-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.30.3",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo lint && turbo test && turbo build",
    "start": "pnpm --filter @jinho-blog/web run start",
    "registry": "pnpm --filter @jinho-blog/web run registry",
    "lint": "turbo lint",
    "lf": "turbo lf",
    "eslint": "turbo eslint",
    "type-check": "turbo type-check",
    "test": "turbo test",
    "kill-port": "for /f \"tokens=5\" %a in ('netstat -ano ^| findstr :3401 ^| findstr LISTENING') do taskkill /F /PID %a"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "baseline-browser-mapping": "^2.9.19",
    "cross-env": "^7.0.3",
    "prettier": "^3.6.2",
    "turbo": "^2.8.10",
    "typescript": "^5",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.7",
      "@types/react-dom": "19.2.3"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}
