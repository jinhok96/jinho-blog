{
  "name": "@jinho-blog/mdx-handler",
  "version": "1.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "lint": "concurrently \"npm run lf\" \"npm run eslint\" \"npm run type-check\"",
    "lf": "prettier --write -l --end-of-line lf src",
    "eslint": "eslint src --fix --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@jinho-blog/shared": "*",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.7.4",
    "typescript": "^5"
  }
}
